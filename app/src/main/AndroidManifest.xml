            </intent-filter>
        </activity>
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.RegisterActivity" />
        <activity android:name=".Activity.UserCenterActivity" />
        <activity android:name=".Activity.UserDataActivity" />
